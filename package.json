{
	"name": "vscode-gtirb-loader",
	"displayName": "GTIRB Loader",
	"description": "Load GTIRB files",
	"version": "0.0.1",
	"publisher": "grammatech",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://git.grammatech.com/tneale/vscode-gtirb-loader"
	},
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCustomEditor:gtirb-loader.gtirb"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "gtirb-loader.gtirb",
				"displayName": "GTIRB",
				"selector": [
					{
						"filenamePattern": "*.gtirb"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.3.5"
	}
}
